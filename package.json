{
  "name": "polysmith",
  "version": "1.0.0",
  "description": "A tool to generate STL and OBJ files for 3D printing.",
  "main": "app/electron.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "electron-builder"
  },
  "build": {
    "appId": "Polysmith",
    "productName": "Polysmith",
    "files": [
      "**/*"
    ],
    "directories": {
      "output": "builds"
    },
    "linux": {
      "target": "deb",
      "icon": "imgs/theme/logo/basic/small/colored.png"
    },
    "win": {
      "target": "nsis",
      "icon": "imgs/theme/logo/basic/small/colored.ico"
    },
    "mac": {
      "target": "dmg",
      "icon": "imgs/theme/logo/basic/small/colored.icns"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jgphilpott/polysmith"
  },
  "keywords": [
    "Polysmith",
    "3D Printing",
    "CSG",
    "STL",
    "OBJ"
  ],
  "author": {
    "name": "Jacob Philpott",
    "email": "jacob.philpott@gmx.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jgphilpott/polysmith/issues"
  },
  "homepage": "https://github.com/jgphilpott/polysmith#readme",
  "dependencies": {
    "coffeescript": "^2.7.0",
    "express": "^4.21.0",
    "nodemon": "^3.1.7",
    "pug": "^3.0.3"
  },
  "devDependencies": {
    "electron": "^32.1.2",
    "electron-builder": "^25.0.5"
  }
}
