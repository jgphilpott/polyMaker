{
  "workbench.colorTheme": "Default High Contrast",
  "editor.scrollBeyondFirstLine": false,
  "editor.scrollBeyondLastLine": false,
  "editor.renderWhitespace": "all",
  "git.decorations.enabled": true,
  "explorer.sortOrder": "default",
  "folder-path-color.folders": [],
  "cSpell.language": "en-US",
  "glassit.alpha": 210,
  "cSpell.words": [
    "collada",
    "cura",
    "forkme",
    "fullscreen",
    "gcode",
    "gltf",
    "hedrons",
    "kebabify",
    "magine",
    "metabox",
    "octree",
    "pascalize",
    "petg",
    "pinshape",
    "polyconvert",
    "polygen",
    "polyslice",
    "polysmith",
    "pydash",
    "snakify",
    "thingiverse",
    "titlize",
    "urandom",
    "vrml"
  ],
  "files.exclude": {
    "builds": true,
    "node_modules": true,
    "**/bundle.js.tmp-browserify-*": true,
    "app/**/*.html": true,
    "app/**/*.css": true,
    "app/**/*.map": true,
    "app/**/*.js": true,
  }
}